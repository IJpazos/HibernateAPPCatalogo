<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.ipartek.formacion.HibernateWeb.listeners.Inicializacion</listener-class>
  </listener>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>com.ipartek.formacion.HibernateWeb.servlets.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControladorCRUD</servlet-name>
  	<servlet-class>com.ipartek.formacion.HibernateWeb.servlets.ControladorCRUD</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorCRUD</servlet-name>
  	<url-pattern>/ControladorCRUD</url-pattern>
  </servlet-mapping>
</web-app>
