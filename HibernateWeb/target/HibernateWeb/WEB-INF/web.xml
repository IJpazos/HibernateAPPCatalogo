<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.ipartek.formacion.HibernateWeb.listeners.Inicializacion</listener-class>
  </listener>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.HibernateWeb.servlets.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControladorCRUD</servlet-name>
  	<display-name>ControladorCRUD</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.HibernateWeb.servlets.ControladorCRUD</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorCRUD</servlet-name>
  	<url-pattern>/ControladorCRUD</url-pattern>
  </servlet-mapping>
</web-app>
